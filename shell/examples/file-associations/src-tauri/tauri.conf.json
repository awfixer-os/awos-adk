{
  "$schema": "../../../crates/tauri-cli/config.schema.json",
  "identifier": "com.tauri.dev-file-associations-demo",
  "build": {
    "frontendDist": ["../index.html"]
  },
  "app": {
    "security": {
      "csp": "default-src 'self'",
      "assetProtocol": {
        "enable": true
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../../.icons/32x32.png",
      "../../.icons/128x128.png",
      "../../.icons/128x128@2x.png",
      "../../.icons/icon.icns",
      "../../.icons/icon.ico"
    ],
    "fileAssociations": [
      {
        "ext": ["png"],
        "mimeType": "image/png",
        "rank": "Default"
      },
      {
        "ext": ["jpg", "jpeg"],
        "mimeType": "image/jpeg",
        "rank": "Alternate"
      },
      {
        "ext": ["gif"],
        "mimeType": "image/gif",
        "rank": "Owner"
      },
      {
        "ext": ["taurijson"],
        "exportedType": {
          "identifier": "com.tauri.dev-file-associations-demo.taurijson",
          "conformsTo": ["public.json"]
        }
      },
      {
        "ext": ["taurid"],
        "exportedType": {
          "identifier": "com.tauri.dev-file-associations-demo.tauridata",
          "conformsTo": ["public.data"]
        }
      }
    ]
  }
}
